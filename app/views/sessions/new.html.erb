<h1>Login</h1>

<%= form_with do |form| %>
    <div class="mb-2">
        <%= form.label :email %>
        <%= form.text_field :email %>
    </div>
    <div class="mb-2">
        <%= form.label :password %>
        <%= form.password_field :password %>
    </div>
    
        <%= form.submit "Login" %>
<% end %>
